{
  "name": "dependencyinjection-asyncextras",
  "description": "",
  "title": "DependencyInjection Async Extras",
  "version": "0.0.0",
  "ciEnvironment": {
    "variables": [
      {
        "name": "NUGET_API_KEY",
        "description": "NuGet API key",
        "required": true,
        "secret": true,
        "defaultValue": null
      },
      {
        "name": "NUGET_SOURCE",
        "description": "NuGet Source (e.g., https://api.nuget.org/v3/index.json)",
        "required": true,
        "secret": false,
        "defaultValue": "https://api.nuget.org/v3/index.json"
      }
    ]
  }
}
